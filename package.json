{
  "name": "yt-channel-gen",
  "version": "0.1.3",
  "description": "Get all uploads from a YouTube channel as an Iterable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run check && npm run lint",
    "check": "npm run build -- --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "build-dev": "npm run build -- --sourceMap",
    "watch": "tsc --watch",
    "dev": "npm run watch -- --sourceMap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolPants/yt-channel-gen.git"
  },
  "keywords": [
    "youtube",
    "channel",
    "iterator",
    "iterable",
    "generator",
    "feed"
  ],
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lolPants/yt-channel-gen/issues"
  },
  "homepage": "https://github.com/lolPants/yt-channel-gen#readme",
  "devDependencies": {
    "@types/node": "^10.12.24",
    "@types/node-fetch": "^2.1.6",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "node-fetch": "^2.3.0"
  }
}
